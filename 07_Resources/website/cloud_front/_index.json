{
	"CloudFront": {
		"Type": "AWS::CloudFront::Distribution",
		"Properties": {
			"DistributionConfig": {
				"Comment": "podcast.0x4447.com",
				"DefaultCacheBehavior": {
					"TargetOriginId": { "Fn::Sub": "podcast.0x4447.com.s3-website-${AWS::Region}.amazonaws.com" },
					"ViewerProtocolPolicy": "redirect-to-https",
					"AllowedMethods": ["GET", "HEAD"],
					"CachedMethods": ["GET", "HEAD"],
					"Compress": true,
					"SmoothStreaming": false,
					"CachePolicyId": { "Ref": "CloudFrontCachePolicy" }
				},
				"Enabled": true,
				"Origins": [{
					"DomainName": { "Fn::Sub": "podcast.0x4447.com.s3-website-${AWS::Region}.amazonaws.com" },
					"Id": { "Fn::Sub": "podcast.0x4447.com.s3-website-${AWS::Region}.amazonaws.com" },
					"CustomOriginConfig": {
						"HTTPPort": 80,
						"HTTPSPort": 443,
						"OriginProtocolPolicy": "http-only",
						"OriginSSLProtocols": ["TLSv1.2"]
					}
				}],
				"Aliases": [ "podcast.0x4447.com" ],
				"ViewerCertificate": {
					"AcmCertificateArn" : { "Ref": "CertificateManager" },
					"SslSupportMethod": "sni-only"
				},
				"DefaultRootObject": "index.html",
				"HttpVersion": "http2",
				"IPV6Enabled": true,
				"PriceClass": "PriceClass_All",
				"Restrictions": {
					"GeoRestriction": {
						"RestrictionType": "blacklist",
						"Locations": [ "CN", "LA", "CU", "VN", "KP" ]
					}
				}
			}
		}
	}
}
