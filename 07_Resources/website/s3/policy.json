{
	"S3WebpagePolicy": {
		"Type": "AWS::S3::BucketPolicy",
		"Properties": {
			"Bucket": { "Ref": "S3Webpage" },
			"PolicyDocument": {
				"Version": "2012-10-17",
				"Statement": [
					{
						"Effect": "Allow",
						"Principal": "*",
						"Action": "s3:GetObject",
						"Resource": { "Fn::Sub": "arn:aws:s3:::${S3Webpage}/*" }
					}
				]
			}
		}
	}
}
